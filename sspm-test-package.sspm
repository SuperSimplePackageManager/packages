name="SSPM Test Package"
description="A simple test package"
source=https://github.com/SuperSimplePackageManager/sspm-test-package/archive/refs/tags/v1.0.tar.gz
format="tar"
sspm-test-package(){
    if [ "$1" == "INSTALL" ]; then
        make install
    elif [ "$1" == "REMOVE" ]; then
        make remove
    fi
}